{
  "common": {
    "tboard-path": "tboard-runs/uci-data-benchmark",
    "checkpoint-path": "checkpoints/uci-data-benchmark",
    "save-checkpoint": false,
    "num-epochs": 3,
    "device": "cuda",
    "num-workers": 2,
    "patience-threshold": 1e-3,
    "early-stop-patience": 30
  },
  "datasets": ["gas", "miniboone"],
  "grid": {
    "common": {
      "region-graph": "rnd-bt",
      "region-graph-sd": true,
      "batch-size": 512,
      "optimizer": "Adam",
      "verbose": true
    },
    "models": {
      "gas": {
        "SOS": {
          "real": {
            "learning-rate": 1e-3,
            "num-components,num-units,num-input-units": [[8, 256, 256]]
          }
        }
      },
      "miniboone": {
        "SOS": {
          "real": {
            "learning-rate": 1e-3,
            "num-components,num-units,num-input-units": [[8, 64, 64]]
          }
        }
      }
    }
  }
}
