{
  "common": {
    "tboard-path": "tboard-runs/uci-data-complex",
    "checkpoint-path": "checkpoints/uci-data-complex",
    "save-checkpoint": false,
    "num-epochs": 1000,
    "device": "cuda",
    "num-workers": 2,
    "patience-threshold": 1e-3,
    "early-stop-patience": 25
  },
  "datasets": ["miniboone", "hepmass", "gas", "power"],
  "grid": {
    "common": {
      "region-graph": "rnd-bt",
      "region-graph-sd": true,
      "batch-size": 512,
      "optimizer": "Adam",
      "verbose": true,
      "num-components": 1
    },
    "models": {
      "power": {
        "MPC": {
          "num-units": 256,
          "num-input-units": 256,
          "learning-rate": 0.001
        },
        "SOS": {
          "real": {
            "num-units": 256,
            "num-input-units": 256,
            "complex": false,
            "learning-rate": 0.001
          },
          "complex": {
            "num-units": 128,
            "num-input-units": 256,
            "complex": true,
            "learning-rate": 0.001
          }
        }
      },
      "gas": {
        "MPC": {
          "num-units": 256,
          "num-input-units": 256,
          "learning-rate": 0.001
        },
        "SOS": {
          "real": {
            "num-units": 256,
            "num-input-units": 256,
            "complex": false,
            "learning-rate": 0.001
          },
          "complex": {
            "num-units": 128,
            "num-input-units": 256,
            "complex": true,
            "learning-rate": 0.001
          }
        }
      },
      "hepmass": {
        "MPC": {
          "num-units": 128,
          "num-input-units": 128,
          "learning-rate": 0.001
        },
        "SOS": {
          "real": {
            "num-units": 128,
            "num-input-units": 128,
            "complex": false,
            "learning-rate": 0.001
          },
          "complex": {
            "num-units": 64,
            "num-input-units": 128,
            "complex": true,
            "learning-rate": 0.001
          }
        }
      },
      "miniboone": {
        "MPC": {
          "num-units": 128,
          "num-input-units": 64,
          "learning-rate": 0.001
        },
        "SOS": {
          "real": {
            "num-units": 128,
            "num-input-units": 64,
            "complex": false,
            "learning-rate": 0.001
          },
          "complex": {
            "num-units": 64,
            "num-input-units": 64,
            "complex": true,
            "learning-rate": 0.001
          }
        }
      }
    }
  }
}
