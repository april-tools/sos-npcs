{
  "common": {
    "tboard-path": "tboard-runs/uci-data-exp-sos",
    "checkpoint-path": "checkpoints/uci-data-exp-sos",
    "save-checkpoint": false,
    "num-epochs": 1000,
    "device": "cuda",
    "num-workers": 2,
    "patience-threshold": 1e-3,
    "early-stop-patience": 25
  },
  "datasets": ["miniboone", "hepmass"],
  "grid": {
    "common": {
      "region-graph": "rnd-bt",
      "region-graph-sd": true,
      "batch-size": 512,
      "optimizer": "Adam",
      "verbose": true
    },
    "models": {
      "hepmass": {
        "ExpSOS": {
          "real-small": {
            "num-units": 128,
            "num-input-units": 128,
            "mono-num-units": 2,
            "mono-num-input-units": 2,
            "complex": false,
            "learning-rate": 0.001
          },
          "real-large": {
            "num-units": 128,
            "num-input-units": 128,
            "mono-num-units": 16,
            "mono-num-input-units": 16,
            "complex": false,
            "learning-rate": 0.001
          },
          "complex-small": {
            "num-units": 128,
            "num-input-units": 128,
            "mono-num-units": 2,
            "mono-num-input-units": 2,
            "complex": true,
            "learning-rate": 0.001
          },
          "complex-large": {
            "num-units": 64,
            "num-input-units": 128,
            "mono-num-units": 16,
            "mono-num-input-units": 16,
            "complex": true,
            "learning-rate": 0.001
          }
        }
      },
      "miniboone": {
        "ExpSOS": {
          "real-small": {
            "num-units": 32,
            "num-input-units": 32,
            "mono-num-units": 2,
            "mono-num-input-units": 2,
            "complex": false,
            "learning-rate": 0.001
          },
          "real-large": {
            "num-units": 32,
            "num-input-units": 32,
            "mono-num-units": 16,
            "mono-num-input-units": 16,
            "complex": false,
            "learning-rate": 0.001
          },
          "complex-small": {
            "num-units": 16,
            "num-input-units": 32,
            "mono-num-units": 2,
            "mono-num-input-units": 2,
            "complex": true,
            "learning-rate": 0.001
          },
          "complex-large": {
            "num-units": 16,
            "num-input-units": 32,
            "mono-num-units": 16,
            "mono-num-input-units": 16,
            "complex": true,
            "learning-rate": 0.001
          }
        }
      }
    }
  }
}
