{
  "common": {
    "tboard-path": "tboard-runs/crossing-rings-gif",
    "checkpoint-path": "checkpoints/crossing-rings-gif",
    "save-checkpoint": true,
    "log-distribution": true,
    "log-frequency": 500,
    "num-epochs": 1200,
    "device": "cuda",
    "num-workers": 0,
    "patience-threshold": 1e-3,
    "early-stop-patience": 50
  },
  "datasets": ["crossing-rings"],
  "grid": {
    "common": {
      "region-graph": "rnd-bt",
      "batch-size": 64,
      "optimizer": "Adam",
      "verbose": true,
      "learning-rate": 1e-3,
      "seed": 213
    },
    "models": {
      "crossing-rings": {
        "MPC": {
            "num-components,num-units": [[1, 12]]
        },
        "SOS": {
          "real": {
            "num-components,num-units": [[1, 4], [3, 2]],
            "complex": false
          }
        }
      }
    }
  }
}
