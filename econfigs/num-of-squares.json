      "region-graph": "rnd-bt",
      "region-graph-sd": true,
      "batch-size": 512,
      "optimizer": "Adam",
      "verbose": true
    },
    "models": {
      "power": {
        "MonotonicPC": {
          "learning-rate": 1e-3,
          "num-components,num-units,num-input-units": [[1, 256, 512], [4, 178, 128], [16, 106, 32], [64, 58, 8]]
        },
        "BornPC": {
          "real": {
            "learning-rate": 1e-3,
            "num-components,num-units,num-input-units": [[1, 256, 512], [4, 178, 128], [16, 106, 32], [64, 58, 8]]
          }
        }
      },
      "gas": {
        "MonotonicPC": {
          "learning-rate": 1e-3,
          "num-components,num-units,num-input-units": [[1, 256, 512], [4, 174, 128], [16, 104, 32], [64, 56, 8]]
        },
        "BornPC": {
          "real": {
            "learning-rate": 1e-3,
            "num-components,num-units,num-input-units": [[1, 256, 512], [4, 174, 128], [16, 104, 32], [64, 56, 8]]
