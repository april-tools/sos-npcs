{
  "common": {
    "tboard-path": "tboard-runs/uci-data",
    "checkpoint-path": "checkpoints/uci-data",
    "save-checkpoint": false,
    "num-epochs": 500,
    "device": "cuda",
    "num-workers": 2,
    "patience-threshold": 1e-3,
    "early-stop-patience": 10
  },
  "datasets": ["power", "gas", "hepmass", "miniboone"],
  "grid": {
    "common": {
      "region-graph": "random",
      "region-graph-sd": true,
      "splines": false,
      "batch-size": 512,
      "optimizer": "Adam",
      "verbose": true,
      "init-method": "uniform",
      "init-scale": 1.0
    },
    "models": {
      "power": {
        "MonotonicPC": {
          "learning-rate": 1e-3,
          "num-replicas,num-units,num-input-units": [[1, 256, 512], [4, 178, 128], [16, 106, 32], [64, 58, 8]]
        },
        "BornPC": {
          "real": {
            "learning-rate": 1e-3,
            "num-replicas,num-units,num-input-units": [[1, 256, 512], [4, 178, 128], [16, 106, 32], [64, 58, 8]]
          }
        }
      },
      "gas": {
        "MonotonicPC": {
          "learning-rate": 1e-3,
          "num-replicas,num-units,num-input-units": [[1, 256, 512], [4, 174, 128], [16, 104, 32], [64, 56, 8]]
        },
        "BornPC": {
          "real": {
            "learning-rate": 1e-3,
            "num-replicas,num-units,num-input-units": [[1, 256, 512], [4, 174, 128], [16, 104, 32], [64, 56, 8]]
          }
        }
      },
      "hepmass": {
        "MonotonicPC": {
          "learning-rate": 1e-3,
          "num-replicas,num-units,num-input-units": [[1, 128, 256], [4, 86, 64], [16, 50, 16], [64, 26, 4]]
        },
        "BornPC": {
          "real": {
            "learning-rate": 1e-3,
            "num-replicas,num-units,num-input-units": [[1, 128, 256], [4, 86, 64], [16, 50, 16], [64, 26, 4]]
          }
        }
      },
      "miniboone": {
        "MonotonicPC": {
          "learning-rate": 1e-3,
          "num-replicas,num-units,num-input-units": [[1, 64, 256], [4, 48, 64], [16, 28, 16], [64, 16, 4]]
        },
        "BornPC": {
          "real": {
            "learning-rate": 1e-3,
            "num-replicas,num-units,num-input-units": [[1, 64, 256], [4, 48, 64], [16, 28, 16], [64, 16, 4]]
          }
        }
      }
    }
  }
}
