{
  "common": {
    "tboard-path": "tboard-runs/uci-data",
    "checkpoint-path": "checkpoints/uci-data",
    "save-checkpoint": true,
    "num-epochs": 1200,
    "device": "cuda",
    "num-workers": 4,
    "patience-threshold": 1e-3,
    "early-stop-patience": 25
  },
  "datasets": ["power", "gas", "hepmass", "miniboone", "bsds300"],
  "grid": {
    "common": {
      "region-graph": "random",
      "depth": -1,
      "splines": [false, true],
      "spline-order": 2,
      "optimizer": "Adam",
      "verbose": true,
      "init-scale": 1.0,
      "num-replicas,num-units": [[1, 256], [4, 128], [16, 64], [64, 32], [256, 16], [1024, 8]],
      "learning-rate": 0.01
    },
    "models": {
      "power": {
        "MonotonicPC": {
          "init-method": "uniform",
          "spline-knots": 256,
          "batch-size": 8192
        },
        "BornPC": {
          "non-monotonic": {
            "init-method": "uniform",
            "exp-reparam": false,
            "spline-knots": 256,
            "batch-size": 8192
          }
        }
      },
      "gas": {
        "MonotonicPC": {
          "init-method": "uniform",
          "spline-knots": 512,
          "batch-size": 4096
        },
        "BornPC": {
          "non-monotonic": {
            "init-method": "uniform",
            "exp-reparam": false,
            "spline-knots": 512,
            "batch-size": 4096
          }
        }
      },
      "hepmass": {
        "MonotonicPC": {
          "init-method": "uniform",
          "spline-knots": 128,
          "batch-size": 2048
        },
        "BornPC": {
          "non-monotonic": {
            "init-method": "uniform",
            "exp-reparam": false,
            "spline-knots": 128,
            "batch-size": 2048
          }
        }
      },
      "miniboone": {
        "MonotonicPC": {
          "init-method": "uniform",
          "spline-knots": 64,
          "batch-size": 1024
        },
        "BornPC": {
          "non-monotonic": {
            "init-method": "uniform",
            "exp-reparam": false,
            "spline-knots": 64,
            "batch-size": 1024
          }
        }
      },
      "bsds300": {
        "MonotonicPC": {
          "init-method": "uniform",
          "spline-knots": 256,
          "batch-size": 512
        },
        "BornPC": {
          "non-monotonic": {
            "init-method": "uniform",
            "exp-reparam": false,
            "spline-knots": 256,
            "batch-size": 512
          }
        }
      }
    }
  }
}
